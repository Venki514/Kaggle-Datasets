{
  "name": "HR Analytics Theme 2025",
  "dataColors": [
    "#DBD6D6",
    "#D93B3B",
    "#12B87F",
    "#E69900",
    "#7F8C8D",
    "#2ECC71",
    "#F1C40F",
    "#8E44AD"
  ],
  "foreground": "#131312",
  "background": "#F5F5F5",
  "foregroundNeutralSecondary": "#6C6966",
  "backgroundLight": "#E5E0DA",
  "foregroundNeutralTertiary": "#ABA7A2",
  "backgroundNeutral": "#C6C2BE",
  "tableAccent": "#118DFF",
  "maximum": "#4A2D75",
  "center": "#1AAB40",
  "minimum": "#E645AB",
  "bad": "#E66C37",
  "neutral": "#6B007B",
  "good": "#12239E",
  "visualStyles": {
    "page": {
      "*": {
        "background": [
          {
            "color": {
              "solid": {
                "color": "#F5F5F5"
              }
            }
          }
        ]
      }
    },
    "*": {
      "*": {
        "background": [
          {
            "color": { "solid": { "color": "#FFFFFF" } },
            "transparency": 0
          }
        ],
        "border": [
          {
            "show": true,
            "color": { "solid": { "color": "#D0D0D0" } },
            "radius": 5
          }
        ],
        "title": [
          {
            "show": true,
            "fontFamily": "Segoe UI Semibold",
            "fontSize": 13,
            "fontColor": { "solid": { "color": "#131312" } },
            "alignment": "Left"
          }
        ],
        "labels": [
          {
            "color": { "solid": { "color": "#6C6966" } },
            "fontSize": 9
          }
        ],
        "categoryAxis": [
          {
            "labelColor": { "solid": { "color": "#6C6966" } },
            "lineColor": { "solid": { "color": "#C6C2BE" } }
          }
        ],
        "valueAxis": [
          {
            "labelColor": { "solid": { "color": "#6C6966" } },
            "lineColor": { "solid": { "color": "#C6C2BE" } }
          }
        ]
      }
    },
    "card": {
      "*": {
        "labels": [
          {
            "color": { "solid": { "color": "#6C6966" } },
            "fontSize": 10
          }
        ],
        "dataLabels": [
          {
            "color": { "solid": { "color": "#131312" } },
            "fontSize": 28,
            "fontFamily": "Segoe UI Light"
          }
        ]
      }
    },
    "columnChart": {
      "*": {
        "dataPoint": [
          {
            "defaultColor": { "solid": { "color": "#DBD6D6" } }
          }
        ],
        "title": [
          {
            "fontSize": 14
          }
        ]
      }
    },
    "stackedColumnChart": {
      "*": {
        "title": [
          {
            "fontSize": 14
          }
        ],
        "dataPoint": [
          {
            "defaultColor": { "solid": { "color": "#DBD6D6" } }
          }
        ]
      }
    },
    "clusteredColumnChart": {
      "*": {
        "title": [
          {
            "fontSize": 14
          }
        ],
        "dataPoint": [
          {
            "defaultColor": { "solid": { "color": "#DBD6D6" } }
          }
        ]
      }
    },
    "barChart": {
      "*": {
        "dataPoint": [
          {
            "defaultColor": { "solid": { "color": "#DBD6D6" } }
          }
        ],
        "title": [
          {
            "fontSize": 14
          }
        ]
      }
    },
    "lineChart": {
      "*": {
        "dataPoint": [
          {
            "defaultColor": { "solid": { "color": "#DBD6D6" } },
            "showMarkers": true
          }
        ],
        "title": [
          {
            "fontSize": 14
          }
        ],
        "lineStyles": [
          {
            "stroke": { "solid": { "color": "#DBD6D6" } },
            "strokeWidth": 2
          }
        ]
      }
    },
    "donutChart": {
      "*": {
        "legend": [
          {
            "position": "Right"
          }
        ],
        "title": [
          {
            "fontSize": 14
          }
        ]
      }
    },
    "slicer": {
      "*": {
        "items": [
          {
            "fontColor": { "solid": { "color": "#131312" } },
            "background": { "solid": { "color": "#FFFFFF" } },
            "fontSize": 10
          }
        ],
        "header": [
          {
            "fontColor": { "solid": { "color": "#6C6966" } },
            "background": { "solid": { "color": "#F5F5F5" } },
            "fontSize": 12,
            "fontFamily": "Segoe UI Bold"
          }
        ],
        "general": [
          {
            "outline": "Frame"
          }
        ]
      }
    },
    "multiRowCard": {
      "*": {
        "card": [
          {
            "outlineColor": { "solid": { "color": "#D0D0D0" } },
            "outlineWeight": 1
          }
        ],
        "categoryLabel": [
          {
            "color": { "solid": { "color": "#6C6966" } },
            "fontSize": 10
          }
        ],
        "valueLabel": [
          {
            "color": { "solid": { "color": "#131312" } },
            "fontSize": 16,
            "fontFamily": "Segoe UI Semibold"
          }
        ]
      }
    },
    "kpi": {
      "*": {
        "goal": [
          {
            "color": { "solid": { "color": "#6C6966" } },
            "fontSize": 10
          }
        ],
        "trend": [
          {
            "color": { "solid": { "color": "#1AAB40" } }
          }
        ],
        "status": [
          {
            "increase": { "solid": { "color": "#1AAB40" } },
            "decrease": { "solid": { "color": "#D93B3B" } },
            "neutral": { "solid": { "color": "#6B007B" } }
          }
        ]
      }
    }
  }
}