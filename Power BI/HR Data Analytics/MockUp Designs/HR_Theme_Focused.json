{
  "name": "HR Analytics Focused Theme",
  "dataColors": [
    "#01579B",
    "#D93B3B",
    "#12B87F",
    "#E69900",
    "#7F8C8D",
    "#2ECC71",
    "#F1C40F",
    "#8E44AD"
  ],
  "background": "#F5F5F5",
  "foreground": "#252423",
  "tableAccent": "#01579B",
  "visualStyles": {
    "*": {
      "*": {
        "background": [
          {
            "color": { "solid": { "color": "#FFFFFF" } },
            "transparency": 0
          }
        ],
        "border": [
          {
            "show": true,
            "color": { "solid": { "color": "#D0D0D0" } },
            "radius": 5
          }
        ],
        "labels": [
          {
            "color": { "solid": { "color": "#605E5C" } },
            "fontFamily": "Segoe UI",
            "fontSize": 9
          }
        ],
        "categoryAxis": [
          {
            "showAxisTitle": false,
            "labelColor": { "solid": { "color": "#605E5C" } },
            "fontSize": 9,
            "fontFamily": "Segoe UI",
            "lineColor": { "solid": { "color": "#C8C6C4" } }
          }
        ],
        "valueAxis": [
          {
            "showAxisTitle": false,
            "labelColor": { "solid": { "color": "#605E5C" } },
            "fontSize": 9,
            "fontFamily": "Segoe UI",
            "lineColor": { "solid": { "color": "#C8C6C4" } }
          }
        ],
        "legend": [
          {
            "show": true,
            "position": "Top",
            "showTitle": false,
            "labelColor": { "solid": { "color": "#605E5C" } },
            "fontFamily": "Segoe UI",
            "fontSize": 9
          }
        ],
        "title": [
          {
            "show": true,
            "fontFamily": "Segoe UI Semibold",
            "fontSize": 13,
            "fontColor": { "solid": { "color": "#252423" } },
            "background": { "solid": { "color": "#FFFFFF" } },
            "alignment": "Left"
          }
        ]
      }
    },
    "card": {
      "*": {
        "labels": [
          {
            "color": { "solid": { "color": "#605E5C" } },
            "fontSize": 10,
            "fontFamily": "Segoe UI"
          }
        ],
        "dataLabels": [
          {
            "color": { "solid": { "color": "#252423" } },
            "fontSize": 28,
            "fontFamily": "Segoe UI Light"
          }
        ],
        "card": [
          {
            "properties": {
              "Measure": {
                "dataColors": {
                  "valueMapping": {
                    "Total Employees": { "solid": { "color": "#01579B" } },
                    "Attrition Rate": { "solid": { "color": "#D93B3B" } },
                    "Total Attrition": { "solid": { "color": "#252423" } },
                    "Avg Job Satisfaction": { "solid": { "color": "#12B87F" } },
                    "Avg Env Satisfaction": { "solid": { "color": "#12B87F" } },
                    "Avg Years At Co.": { "solid": { "color": "#252423" } },
                    "Avg Work Life Balance": { "solid": { "color": "#E69900" } },
                    "Avg Performance": { "solid": { "color": "#01579B" } },
                    "Avg Monthly Income": { "solid": { "color": "#252423" } }
                  }
                }
              }
            }
          }
        ]
      }
    },
    "gauge": {
      "*": {
        "gaugeSecondaryAxis": [
          {
            "max": 4,
            "min": 1,
            "colors": {
              "fill": {
                "valueMapping": {
                  "Avg Job Satisfaction": { "solid": { "color": "#12B87F" } },
                  "Avg Env Satisfaction": { "solid": { "color": "#12B87F" } },
                  "Avg Work Life Balance": { "solid": { "color": "#E69900" } },
                  "Avg Performance Rating": { "solid": { "color": "#01579B" } }
                }
              }
            }
          }
        ],
        "labels": [
          {
            "color": { "solid": { "color": "#605E5C" } }
          }
        ],
        "dataLabels": [
          {
            "color": { "solid": { "color": "#252423" } },
            "fontSize": 28,
            "fontFamily": "Segoe UI Light"
          }
        ],
         "visualHeader": [ { "show": false } ]
      }
    },
    "slicer": {
      "*": {
        "items": [
          {
            "fontColor": { "solid": { "color": "#252423" } },
            "background": { "solid": { "color": "#FFFFFF" } },
            "fontSize": 10,
            "fontFamily": "Segoe UI"
          }
        ],
        "header": [
          {
            "fontColor": { "solid": { "color": "#605E5C" } },
            "background": { "solid": { "color": "#F5F5F5" } },
            "fontSize": 12,
            "fontFamily": "Segoe UI Bold"
          }
        ],
        "general": [
          {
             "properties": {
                "outlineColor": {"solid": {"color": "#D0D0D0"}},
                "outlineWeight": 1
             }
          }
        ]
      }
    },
    "matrixV2": {
      "*": {
        "grid": [
          {
            "gridVertical": true,
            "gridVerticalColor": { "solid": { "color": "#EAEAEA" } },
            "gridVerticalWeight": 1,
            "gridHorizontal": true,
            "gridHorizontalColor": { "solid": { "color": "#EAEAEA" } },
            "gridHorizontalWeight": 1,
            "rowPadding": 2,
            "outlineColor": { "solid": { "color": "#D0D0D0" } },
            "outlineWeight": 1
          }
        ],
        "columnHeaders": [
          {
            "fontColor": { "solid": { "color": "#252423" } },
            "backColor": { "solid": { "color": "#F5F5F5" } },
            "fontSize": 10,
            "fontFamily": "Segoe UI Semibold"
          }
        ],
        "rowHeaders": [
          {
            "fontColor": { "solid": { "color": "#252423" } },
            "backColor": { "solid": { "color": "#FFFFFF" } },
            "fontSize": 10,
            "fontFamily": "Segoe UI"
          }
        ],
        "values": [
          {
            "fontColorPrimary": { "solid": { "color": "#252423" } },
            "backColorPrimary": { "solid": { "color": "#FFFFFF" } },
            "fontSize": 10,
            "fontFamily": "Segoe UI"
          }
        ]
      }
    },
    "barChart": {
      "*": {
        "dataPoint": [
          {
            "defaultColor": { "solid": { "color": "#C8C6C4" } }
          }
        ]
      }
    },
    "columnChart": {
      "*": {
        "dataPoint": [
          {
            "defaultColor": { "solid": { "color": "#C8C6C4" } }
          }
        ]
      }
    },
     "lineChart": {
      "*": {
        "dataPoint": [
          {
            "showMarkers": true
          }
        ]
      }
    },
    "donutChart": {
      "*": {
        "labels": [
          {
            "color": { "solid": { "color": "#252423" } },
            "fontSize": 9
          }
        ],
        "legend": [ { "show": false } ]
      }
    },
     "scatterChart": {
      "*": {
        "legend": [ { "show": true } ],
        "dataPoint": [ { "showMarkers": true } ]
      }
    },
    "treeMap": {
      "*": {
         "dataLabels": [
           {
             "color": { "solid": { "color": "#FFFFFF" } },
             "fontSize": 10
           }
         ],
         "legend": [ { "show": false } ]
      }
    }
  }
}